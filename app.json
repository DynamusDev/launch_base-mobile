{
  "expo": {
    "name": "template-mobile",
    "slug": "template-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/new_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dynamusdev.templatemobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/new_icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.dynamusdev.templatemobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
