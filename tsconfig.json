{
  "compilerOptions": {
    "jsx": "react-native",
    "target": "esnext",
    "lib": ["esnext", "es2020"],
    "module": "esnext",
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "extends": "expo/tsconfig.base"
}
